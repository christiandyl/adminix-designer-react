{
  "name": "adminix-designer-react",
  "description": "Adminix embeddable designer for React.js",
  "author": "Christian Dyl",
  "keywords": [
    "react",
    "workflows",
    "ui"
  ],
  "version": "0.2.3",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christiandyl/adminix-designer-react.git"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "scripts": {
    "start": "PORT=8000 ./node_modules/.bin/react-scripts start",
